<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true" scanPeriod="10 seconds">

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>XLS2JSON %d{dd/MM/yyyy HH:mm:ss} %m%n</pattern>
        </encoder>
    </appender>

    <logger name="com.renault.eplaform.core.xls2json" level="DEBUG"/>


    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
